<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>rtf-odf-scan-for-zotero@mystery-lab.com</em:id>
    <em:type>2</em:type>
    <em:name>ODF Scan for Zotero</em:name>
    <em:version>2.0.44</em:version>
    <em:creator>Frank Bennett</em:creator>
    <em:contributor/>
    <em:description>Adds convertible citation marker support to Zotero.</em:description>
    <em:aboutURL>chrome://rtf-odf-scan-for-zotero/content/about.xul</em:aboutURL>
    <em:optionsURL>chrome://rtf-odf-scan-for-zotero/content/options.xul</em:optionsURL>
    <em:updateURL>https://juris-m.github.io/zotero-odf-scan-plugin/update.rdf</em:updateURL>
    <em:bootstrap>true</em:bootstrap>
    <em:unpack>true</em:unpack>
    <em:targetApplication>
      <Description>
        <em:id>zotero@chnm.gmu.edu</em:id>
        <!-- Zotero -->
        <em:minVersion>4.0</em:minVersion>
        <em:maxVersion>5.0.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    <em:targetApplication>
      <Description>
        <!-- Juris-M -->
        <em:id>juris-m@juris-m.github.io</em:id>
        <em:minVersion>4.0</em:minVersion>
        <em:maxVersion>5.0.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
